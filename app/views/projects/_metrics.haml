%td{:valign => "botom", :align => "right"}
  - if can? :read, project
    = link_to project.name, project_path(project.id)
  - else
    = project.name
- project.latest_metric_samples(metric_names).each do |sample|
  %td
    - if !sample.nil?
      %a.detail-link{href:"/projects/#{project.id}/metrics/#{sample.metric_name}/detail", style: 'width:100%;height:100%;'}
        %div.chart_place{:id => "project-#{project.id}-#{@display_type}-#{sample.try(:metric_name)}"}
